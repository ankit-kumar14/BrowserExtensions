{
    "manifest_version": 3,
    "name": "Greeting Extension",
    "version": "1.0",
    "description": "Displays a greeting message when a new tab is opened.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "chrome_url_overrides": {
      "newtab": "newTab.html"
    }
  }
  